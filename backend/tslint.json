{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-airbnb"],
  "jsRules": {},
  "rules": {
    "max-line-length": [
      true,
      {
        "limit": 80,
        "ignore-pattern": "^import |^export {(.*?)}"
      }
    ],
    "variable-name": [false],
    "ter-arrow-parens": [false]
  },
  "rulesDirectory": []
}
